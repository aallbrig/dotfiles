Host github.com
    HostName github.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/ssh-ed25519

# General Purpose Work GitHub Identity Reference
Host github.com
    HostName github.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/id_rsa_work

Host *amazonaws.com
    IdentityFile ~/.ssh/andrew-allbright-keypair.pem
    User ec2-user

Host *
    AddKeysToAgent yes
    IdentityFile ~/.ssh/ssh-ed25519
